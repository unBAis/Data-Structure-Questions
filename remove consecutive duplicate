def good_str(val):
    new = ''
    i = 0
    j = 0
    while j < len(val):
        if val[i] == val[j]:
            j += 1
        elif (val[i] != val[j]) or (j == len(val)-1):
            new += val[i]
            i = j
            j += 1
    new += val[j-1]
    return new


#for input use:
num = int(input())
for i in range(num):
    txt = input()
    print(good_str(txt))
